{
  "name": "@sirmrmarty/n8n-nodes-gemini-cli",
  "version": "0.1.2",
  "description": "n8n node for Google Gemini CLI integration with AI-powered coding assistance",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "gemini",
    "gemini-cli",
    "ai",
    "google",
    "google-ai",
    "google-gemini",
    "vertex-ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sirmrmarty/n8n-nodes-geminicli#readme",
  "bugs": {
    "url": "https://github.com/sirmrmarty/n8n-nodes-geminicli/issues"
  },
  "author": {
    "name": "SirMrMarty"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sirmrmarty/n8n-nodes-geminicli.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && npx tsc && npx gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npx eslint -c .eslintrc.prepublish.js nodes/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/GeminiCli/GeminiCli.node.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "~8.32.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}